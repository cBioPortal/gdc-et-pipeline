<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<mapping>
    <class name="org.cbio.gdcpipeline.model.gdc.clinical.GDCClinicalDataModel">
       <map-to xml="patient"/>

       <field name="patient_id" type="string">
          <bind-xml name="bcr_patient_barcode" node="element"/>
       </field>

      <!-- <field name="sample_id" type="string">
          <bind-xml name="quantity" node="element"/>
       </field> -->
        <field name="sex" type="string">
            <bind-xml name="gender" node="element"/>
        </field>
        <field name="age" type="integer">
            <bind-xml name="age_at_initial_pathologic_diagnosis" node="element"/>
        </field>
        <field name="os_status" type="string">
            <bind-xml name="vital_status" node="element"/>
        </field>
    </class>
</mapping>